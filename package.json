  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-multi": "^1.0.1",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bookshelf": "^0.13.3",
